public class Ep1CallingMethods {
    public static void main(String[] args) throws Exception {
        int i = 5;
        int j = 2;
        int k = max(i, j);
        System.out.println("The maximum of " + i + " and " + j + " is " + k);

    }

    public static int max(int i, int j) {
        if (i < j)
            return j;
        else
            return i;

    }

}

