public class Checkchar {
    public static void main(String[] args) {
        char c = '8';
        if (c > '0' && c < '9')
            System.out.println(c + " is a numeric character.");
        if (c > 'A' && c < 'Z')
            System.out.println(c + " is an uppercase character");
        if (c > 'a' && c < 'z')
            System.out.println(c + " is a lowercase character");

    }
}
